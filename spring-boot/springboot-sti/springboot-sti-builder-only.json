{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "springboot-sti"
   },
   "items": [
      {
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "springboot-sti"
         },
         "spec": {
            "triggers": [
               {
                  "type": "GitHub",
                  "github": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "Generic",
                  "generic": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "ImageChange",
                  "imageChange": {}
               }
            ],
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/tecong/springboot-on-openshift-examples.git",
                  "ref": "master"
               },
               "contextDir": "spring-boot/springboot-sti"
            },
            "strategy": {
               "type": "Docker",
               "dockerStrategy": {
                  "from": {
                     "kind": "ImageStreamTag",
                     "name": "centos:7"
                  }
               }
            },
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "springboot-sti:latest"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "centos"
         },
         "spec": {
            "dockerImageRepository": "library/centos:centos7"
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "springboot-sti"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest",
                  "annotations": {
                     "description": "Spring Boot STI image",
                     "iconClass": "icon-jboss",
                     "tags": "builder,springboot,java,maven,gradle",
                     "supports": "springboot,java:8,maven:3,gradle:2.6",
                     "version": "1.0"
                  }
               }
            ]
         }
      }


   ]
}
